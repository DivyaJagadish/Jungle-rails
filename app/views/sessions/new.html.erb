
<h1>Login</h1>


<%= form_for :session, url: '/login' do |form| %>

  <div class="form-group">
  <%= form.label :"Email" %>
  <%= form.text_field :email, class: 'form-control' %>
 </div>
 <div class="form-group">
  <%= form.label :"Password" %>
  <%= form.text_field :password, class: 'form-control' %>
</div>
 <div class="form-group">
  <%= form.submit 'Submit', class: 'btn btn-primary' %>
  </div>

<% end %>